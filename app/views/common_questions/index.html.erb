<div class="page-header">
  <h1>Frequently Asked Questions</h1>
</div>
<div>
  <ul>
    <% @faq.each do |question, answer| %>
      <li><%= question %></li>
      <% end %>
  </ul>
</div>
<div class="row">
  <div class="col-md-8">
  <% @faq.each do |question, answer| %>
    <h3><%= question %></h3>
    <p> <%= answer %> </p>
    <p>Top</p>
    <% end %>
  </div>
</div>
