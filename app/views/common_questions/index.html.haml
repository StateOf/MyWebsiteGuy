.page-header
  %h1 Frequently Asked Questions

%div
  %ul
    - @quotes.each do |quote|
      %li= link_to quote.question, "##{quote.slug}"


.container-fluid
  .row
    - @quotes.each do |quote|
      = content_tag :a, '', name: quote.slug
      %h3= quote.question
      %p= quote.answer
      = link_to "top", "#"
